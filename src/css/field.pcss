.player {
  /* background: url('../assets/img/player-01.png') center no-repeat; */
  background-size: contain;
  &:hover,
  &.hover {
    .player__hover {
      display: block;
    }
  }
  &__hover {
    z-index: 10;
    position: absolute;
    width: 300%;
    left: 100%;
    bottom: 100%;
    display: none;
    background-color: #1D1D1B;
    text-align: center;
    padding: 10px;
    @media screen and (max-width: 768px) {
      padding: 20px;
    }
  }
  &__name,
  &__life {
    margin-bottom: 10px;
  }
  &__life {
    font-weight: 700;
  }
  &__role {
    font-weight: 300;
  }
}

.player-profile {
  display: none;
  position: fixed;
  max-width: 600px;
  max-height: 800px;
  width: 90vw;
  height: 90vw;
  z-index: 22;
  top: 50%;
  left: 50%;
  padding: 30px;
  transform: translate(-50%, -50%);
  background: white;
  flex-wrap: wrap;
  align-content: flex-start;
  color: #1D1D1B;
  &.visible {
    display: flex;
  }
  &__close {
    position: absolute;
    right: 0;
    top: 0;
    line-height: 35px;
    font-size: 70px;
    font-weight: 300;
    cursor: pointer;
  }
  &__name {
    flex: 1 0 100%;
    text-align: center;
    margin-bottom: 30px;
  }
  &__about {
    display: flex;
  }
  &__info {
    list-style: none;
    .info-item {
      padding: 20px;
    }
  }
  &__photo {
    display: block;
    flex: 1 0 200px;
    height: 200px;
    display: flex;
    align-items: center;
    justify-content: center;
    border-right: 1px solid black;
    padding-right: 30px;
  }
}

.tab {
  padding: 5px 10px;
  flex: 1 0 25%;
  display: flex;
  position: relative;
  text-align: center;
  align-items: center;
  justify-content: center;
  text-transform: uppercase;
  font-size: 14px;
  cursor: pointer;
  background-color: #F1F1F2;
  &:active,
  &.active {
    background-color: #fff;
  }
  &::before {
    content: '';
    position: absolute;
    right: 0;
    top: 50%;
    transform: translateY(-50%);
    width: 1px;
    height: 50%;
    background: #9C9B9B;
  }
  &:last-child {
    &::before {
      display: none;
    }
  }
}

.aside-field {
  .field {
    margin: 0;
  }
  .player__hover {
    display: none !important;
  }
}

.tabs {
  width: 100%;
  background-color: #F1F1F2;
  &__caption {
    display: flex;
    list-style: none;
    margin: 1px;
  }
  &__content {
  //background-color: #F1F1F2; display: none;
    margin: 20px;

    &.active {
      display: block;
    }
    p {
    //padding: 15px; line-height: 24px;
      font-weight: 300;
      margin-bottom: 10px;
    }
  }
}

.meta-icon__wrapper {
  display: flex;
}

.meta-icon {
  position: relative;
  height: 30px;
  width: 30px;
  margin-right: 20px;
  background-repeat: no-repeat;
  background-position: center;
  background-size: contain;
  cursor: pointer;
  &--sport {
    background-image: url('../assets/img/sport.png');
  }
  &--weapon {
    background-image: url('../assets/img/aim.png');
  }
  &--spion {
    background-image: url('../assets/img/fly.png');
  }
  &--friend {
    background-image: url('../assets/img/friend.png');
  }
  &:hover {
    .meta-icon__hover {
      display: block;
    }
  }
  &__hover {
    top: -60px;
    display: none;
    background: #1D1D1B;
    position: absolute;
    color: white;
    padding: 10px;
    width: 130px;
    text-align: center;
  }
}