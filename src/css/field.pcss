/* .field */

.section-field {
  position: relative;

  height: auto !important;
  padding: 0 240px;
  @media screen and (max-width: 768px) {
    padding: 0 50px;
  }
  .fp-tableCell {
    display: flex;
    align-items: center;
    @media screen and (max-width: 768px) {
      height: auto !important;
    }
  }
}

.field {
  position: relative;
  margin: 30px 0;
  width: 100%;

//top: 50%; //transform: translateY(-50%);

  &__image {
    max-height: 70vh;
  }

  &__player {
    position: absolute;
    transform: translate(-50%, -50%);
    top: 0;
    left: 0;
    width: 10%;
    height: 20%;
    cursor: pointer;
    transform-origin: center;
    background-color: #F2F1F2;
    transition: all 1s ease;
    &.remove {
      filter: contrast(0%) brightness(0%);
    }
    &:hover {
      transform: translate(-50%, -50%) scale(1.1);
    }

    &--1 {
      background: url('../assets/img/player-01.png') center no-repeat;
      top: 50%;
      left: 12%;
      z-index: 15;
    }
    /* column 2 */
    &--2 {
      background: url('../assets/img/player-02.png') center no-repeat;
      top: 25%;
      left: 25%;
      z-index: 14;
    }
    &--3 {
      background: url('../assets/img/player-03.png') center no-repeat;
      top: 50%;
      left: 25%;
      z-index: 13;
    }

    &--4 {
      background: url('../assets/img/player-04.png') center no-repeat;
      top: 75%;
      left: 25%;
      z-index: 12;
    }

    /* column 3  */
    &--5 {
      background: url('../assets/img/player-05.png') center no-repeat;
      top: 33%;
      left: 38%;
      z-index: 11;
    }
    &--6 {
      background: url('../assets/img/player-06.png') center no-repeat;
      top: 66%;
      left: 38%;
      z-index: 10;
    }

    /* column 4  */
    &--7 {
      background: url('../assets/img/player-07.png') center no-repeat;
      top: 33%;
      left: 62%;
      z-index: 9;
    }
    &--8 {
      background: url('../assets/img/player-08.png') center no-repeat;
      top: 66%;
      left: 62%;
      z-index: 9;
    }
    /* column 5  */
    &--9 {
      background: url('../assets/img/player-09.png') center no-repeat;
      top: 25%;
      left: 75%;
      z-index: 8;
    }
    &--10 {
      background: url('../assets/img/player-10.png') center no-repeat;
      top: 50%;
      left: 75%;
      z-index: 7;
    }
    &--11 {
      background: url('../assets/img/player-11.png') center no-repeat;
      top: 75%;
      left: 75%;
      z-index: 6;
    }
    /* column 6  */
    &--12 {
      background: url('../assets/img/player-12.png') center no-repeat;
      top: 50%;
      left: 88%;
      z-index: 4;
    }
  }
  .player[data-profile="starost"] {
    opacity: 0;
    pointer-events: none;
  }
}

