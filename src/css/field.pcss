.player-ava {
  display: inline-block;
  position: relative;
  width: 30px;
  height: 30px;
  background-repeat: no-repeat;
  background-size: cover;
  background-position: center;
  cursor: pointer;
  background-image: url('../assets/img/ava/arhan14.png');
  transition: all 1s ease;
  opacity: 0;
  pointer-events: none;
  &[data-profile="arhan"]{
    background-image: url('../assets/img/ava/arhan14.png');
  }

  &[data-profile="geek"]{
    background-image: url('../assets/img/ava/geek14.png');
  }

  &[data-profile="gvozd"]{
    background-image: url('../assets/img/ava/gvozd14.png');
  }

  &[data-profile="kadril"]{
    background-image: url('../assets/img/ava/kadril14.png');
  }

  &[data-profile="krivonos"]{
    background-image: url('../assets/img/ava/krivonos14.png');
  }

  &[data-profile="ryabo"]{
    background-image: url('../assets/img/ava/ryabo14.png');
  }
  &[data-profile="steb"]{
    background-image: url('../assets/img/ava/steb14.png');
  }

  &[data-profile="strep"]{
    background-image: url('../assets/img/ava/strep14.png');
  }

}

